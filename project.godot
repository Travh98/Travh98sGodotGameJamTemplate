config_version=5

[application]

config/name="Travh98's Godot Game Jam Template"
run/main_scene="res://components/game_tree/game_tree.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameMgr="*res://components/game_mgr/game_mgr.gd"
QuestMgr="*res://components/quest_mgmt/quest_mgr.gd"
UserInterfaceMgr="*res://ui/ui_mgr/user_interface_mgr.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"ExampleChar2": "res://dialog/characters/ExampleChar2.dch",
"Travh98": "res://dialog/characters/Travh98.dch"
}
directories/dtl_directory={
"example_dialog": "res://dialog/timelines/example_dialog.dtl"
}

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

